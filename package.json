{
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "node ./scripts/gen-rss.js && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier -w -u --cache --cache-strategy content pages",
    "lint:md": "markdownlint \"pages/**/*.md\" -f",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "pages/!*.md": "prettier --write",
    "pages/*.md": [
      "markdownlint -c .markdownlint.json -f",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@giscus/react": "^2.2.8",
    "@vercel/analytics": "^1.0.1",
    "gray-matter": "^4.0.2",
    "next": "^13.4.10",
    "nextra": "^2.13.2",
    "nextra-theme-blog": "^2.13.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rss": "^1.2.2"
  },
  "devDependencies": {
    "eslint": "^8.40.0",
    "eslint-config-next": "13.4.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-mdx": "^2.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "markdownlint": "^0.28.2",
    "markdownlint-cli": "^0.34.0",
    "prettier": "2.8.8"
  },
  "packageManager": "yarn@3.5.1"
}
